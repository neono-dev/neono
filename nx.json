{
  "extends": "nx/presets/npm.json",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "nxCloudAccessToken": "M2EzMzYzMTItM2QxNC00YzA2LTkwYWYtZmEyNGVmOTNhMDg5fHJlYWQtd3JpdGU=",
  "namedInputs": {
    "sharedGlobals": [
      "{workspaceRoot}/.github/workflows/ci.yml"
    ]
  },
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ],
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build:watch": {
      "dependsOn": []
    },
    "test": {
      "cache": true,
      "dependsOn": []
    },
    "type-check": {
      "cache": true,
      "dependsOn": []
    }
  }
}